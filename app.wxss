/**app.wxss**/
page {
  background-color: gainsboro;
  height: 100%;
  margin: 0;
  padding: 0;
}
@font-face {font-family: "iconfont";
  src: url('./fonts/iconfont.eot?t=1520601154530'); /* IE9*/
  src: url('iconfont.eot?t=1520601154530#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
  url('./fonts/iconfont.ttf?t=1520601154530') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('./fonts/iconfont.svg?t=1520601154530#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family:"iconfont" !important;  
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-pinpaishixiaoziti:before { content: "\e600"; }

.container {
  width: 100%;
  box-sizing: border-box;
  z-index: 100;
}
.pop {
  width: 100%;
  height: 100%;
  background-color: rgba(139, 135, 132, 0.7);
  position: fixed;
  z-index: 100;
  left: 0;
  top: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}
.down {
  margin-top: 0.26rem;
  width: 100%;
  background-color: #fff;
}

.down_head_title {
  padding: 0.68rem 0 0.43rem 0.82rem;
  font-size: 0.84rem;
  color:	#6B6B6B;
  border-bottom: 1px solid #f5f5f5;
}

.test_item {
  width: 100%;
  margin-top: 0.81rem;
  padding-bottom: 0.44rem;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #f5f5f5;
}

.item_img {
  margin-left: 0.99rem;
  margin-right: 0.64rem;
  width: 3.9rem;
  height: 3.9rem;
  border-radius: 0.3rem;
}

.item_msg:first-child {
  color: #363636;
  font-size: 0.84rem;
}

.item_msg {
  width: 55%;
  color: black;
}

.item_msg1 {
  width:90%;
  color: #363636;
  font-size: 0.84rem;
   white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.item_msg2 {
  padding-top: 0.50rem;
  color: 	#6B6B6B;
  font-size: 0.73rem;
}

.item_msg3 {
  color: 	#6B6B6B;
  padding-top: 0.5rem;
  font-size: 0.73rem;
  width: 80%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.item_button {
  text-align: center;
  margin-right: 0.625rem;
  width: 2.8rem;
  height: 1.39rem;
  line-height: 1.39rem;
  border: 1.5px solid #eb7d8f;
  border-radius: 0.2rem;
  color: #eb7d8f;
  font-size: 0.68rem;
}
.no_content {
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size:13rem 13rem;
  /* background-size: cover;   */
  /* -moz-background-size: 80% 50%;
  -o-background-size: 80% 50%;
  -webkit-background-size: 80% 50%; */
  background-position: 50% 5rem;
  width: 100%;
  min-height: 500px;
  overflow-y: hidden;
}